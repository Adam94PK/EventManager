<h1 id="header1">Followed events</h1>
</br>

<%# counter = 0 %>

	<% if @events.present? %>
		<% @events.each do |event| %>
		<div class="event_content2">
		
		        <div class="panel">
        		  <div class="panel-heading">
        		  	<%= event.name %>
        		  </div>
		          <div class="panel-footer">
		          	When: <%= event.date %> &emsp;
        			Where: <%= event.place %>
          		  </div>
        		</div>

		</div>
		<% end %>
	<% end %>
