<h1 id="header1">All events</h1>

<div class="event_content">
<% counter = 0 %>
<% @events.each do |event| %>
	<p>
	<%= counter+=1 %>
	<%= link_to event.name, event_path(event), class: "btn btn-link-red" %>
	</p>
<% end %>
</div>

